@page
@model Site.Client.Pages.ApartmentsModel
@{
    ViewData["Title"] = "Apartments";
}

<h1>@ViewData["Tİtle"]</h1>

<table class="table table-bordered">
    <thead>
        <tr>
            <th>Id</th>
            <th>Blok</th>
            <th>Floor</th>
            <th>ApartmentNumber</th>
            <th>Status</th>
            <th>Type</th>
            <th>Owner</th>
            <th>UserId</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.ApartmentModel)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Blok</td>
                <td>@item.ApartmentNumber</td>
                <td>@item.Floor</td>
                <td>@item.Owner</td>
                <td>@item.Status</td>
                <td>@item.Type</td>
                <td>@item.UserId</td>
            </tr>
        }
    </tbody>
</table>



